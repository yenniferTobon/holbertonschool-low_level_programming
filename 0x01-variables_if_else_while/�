#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
	int n=110;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	puts("\"hola");
	if (n > 0)
	{
		printf("positivo");
	} else if (n < 0)
	{
		printf("negativo");
	} else 
		printf("cero");
	return (0);
} 
